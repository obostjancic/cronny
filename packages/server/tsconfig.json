{
  "compilerOptions": {
    "strict": true,
    "target": "ES2020",
    "module": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "rootDir": "./src",
    "moduleResolution": "NodeNext",
    "outDir": "./dist",
    "noUnusedLocals": false,
    "noUnusedParameters": false
  }
}
